{
  "name": "funtch",
  "version": "1.3.0",
  "engines": {
    "node": ">=6"
  },
  "description": "FUNctional feTCH isomorphic",
  "browser": "browser.js",
  "main": "index.js",
  "scripts": {
    "prettier": "./node_modules/.bin/prettier --print-width 100 --single-quote --trailing-comma all --color --write \"./src/**/*.js\"",
    "eslint": "./node_modules/.bin/eslint --fix \"./src/**/*.js\"",
    "codestyle": "npm run prettier && npm run eslint",
    "pretest": "npm run codestyle",
    "test": "./node_modules/.bin/nyc --reporter text --reporter lcov --require babel-core/register ./node_modules/.bin/ava",
    "coverage": "./node_modules/.bin/codecov -f ./coverage/lcov.info",
    "doc": "./node_modules/.bin/esdoc",
    "build:browser": "./node_modules/.bin/babel ./src/funtch.js | ./node_modules/.bin/uglifyjs -o browser.js -c -m",
    "build": "npm test && npm run build:browser && npm run doc",
    "postbuild": "node example/cjs.test.js && ./node_modules/.bin/babel-node example/es6.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/funtch.git"
  },
  "keywords": [
    "fetch",
    "isomorphic",
    "functional"
  ],
  "author": "Vincent Boutour",
  "contributors": [
    "Vincent Boutour (https://vibioh.fr)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViBiOh/funtch/issues"
  },
  "homepage": "https://github.com/ViBiOh/funtch",
  "devDependencies": {
    "ava": "latest",
    "babel-cli": "latest",
    "babel-eslint": "latest",
    "babel-preset-es2015": "latest",
    "codecov": "latest",
    "esdoc": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "latest",
    "js-utils": "git+https://github.com/ViBiOh/js-utils.git",
    "nyc": "latest",
    "prettier": "latest",
    "sinon": "latest",
    "uglify-js": "latest"
  },
  "dependencies": {
    "isomorphic-fetch": "latest"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "concurrency": 4,
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "esdoc": {
    "source": "./src",
    "includes": [
      ".js$"
    ],
    "excludes": [
      ".test\\.js$"
    ],
    "destination": "./doc",
    "experimentalProposal": {
      "classProperties": true,
      "objectRestSpread": true,
      "decorators": true,
      "doExpressions": true,
      "functionBind": true,
      "asyncGenerators": true,
      "exportExtensions": true,
      "dynamicImport": true
    }
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "env": {
      "browser": true
    }
  }
}
